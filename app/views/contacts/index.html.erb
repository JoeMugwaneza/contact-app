<h1> Contact List</h1>
<table class="table table-bordered">
    <thead>
      <tr>
        <th>Firstname</th>
        <th>Lastname</th>
        <th>Email</th>
        <th>Phone Number</th>
      </tr>
    </thead>
    <tbody>
      <% @contacts.each do |contact| %>
      <tr>
        <td><%= link_to contact.first_name, "/contacts/#{contact.id}"%></td>
        <td><%= contact.last_name %></td>
        <td><%= contact.email_address %></td>
        <td><%= contact.telephone_number %></td>
      </tr>
        <% end %>      
    </tbody>
  </table>